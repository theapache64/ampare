{
  "name": "Ampare",
  "version": "1.0.2",
  "description": "A Chrome extension to add additional features to Amplitude.",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.amplitude.com/*"
      ],
      "js": [
        "chrome.js"
      ],
      "runat": "document_end"
    }
  ]
}